{
  "name": "tunebuddy",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev --workspace client\" \"npm:dev --workspace server\"",
    "dev:client": "npm run dev --workspace client",
    "dev:server": "npm run dev --workspace server",
    "build": "npm run build --workspace client && npm run build --workspace server"
  },
  "devDependencies": {
    "axios": "^1.13.2",
    "concurrently": "^8.2.2"
  }
}
